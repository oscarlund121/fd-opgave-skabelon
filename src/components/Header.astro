<header class="full-bleed">
  <nav>
    <h3>AskExperts</h3>
    <ul>
      <li>Home</li>
      <li>Case Study</li>
      <li>Team</li>
      <li>About</li>
    </ul>
    <button>Login</button>
  </nav>
</header>

<style>

header {
    background-color: var(--service-card-color);
    padding: 0;
    margin: 0;
  }

  nav {
    display: flex;
    justify-content: space-between;
    align-items: center;
    
    width: 100%;
    color: white;
    font-size: var(--p-font-size);
    font-family: var(--font-family-p);
    h3 {
      font-family: var(--font-family-h);
      font-size: var(--h4-font-size);
      font-weight: bold;
    }

    ul {
      display: flex;
      gap: 20px;
      li {
        list-style: none;
      }
    }

    

  button{
    background: yellow;
    border: none;
    color: black;
    border-radius: 25px;
    padding: 5px 20px;
  }
  }





</style>
